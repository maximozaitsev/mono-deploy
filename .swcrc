{
  "jsc": {
    "target": "es2022",
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": true
    },
    "transform": {
      "react": {
        "runtime": "automatic"
      }
    },
    "loose": false,
    "externalHelpers": false,
    "keepClassNames": false
  },
  "module": {
    "type": "es6",
    "strict": false,
    "strictMode": true,
    "lazy": false,
    "noInterop": false
  },
  "minify": true,
  "env": {
    "targets": {
      "chrome": "90",
      "firefox": "88", 
      "safari": "14",
      "edge": "90"
    },
    "mode": "usage",
    "coreJs": false,
    "shippedProposals": true,
    "include": [],
    "exclude": [
      "es.array.at",
      "es.array.flat",
      "es.array.flat-map",
      "es.object.from-entries",
      "es.object.has-own",
      "es.string.trim-start",
      "es.string.trim-end"
    ]
  }
}
